{
  "name": "midublog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/tailwind": "^4.0.0",
    "@tailwindcss/typography": "^0.5.9",
    "astro": "^2.8.0",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "astro-icon": "^0.8.1",
    "eslint": "^8.44.0",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "typescript": "^5.1.6"
  },
  "prettier": {
    "astroAllowShorthand": true,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ],
    "pluginSearchDirs": [
      "."
    ],
    "plugins": [
      "prettier-plugin-astro"
    ]
  }
}
